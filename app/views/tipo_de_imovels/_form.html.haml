= form_for @tipo_de_imovel do |f|
  - if @tipo_de_imovel.errors.any?
    #error_explanation
      %h2= "#{pluralize(@tipo_de_imovel.errors.count, "error")} prohibited this tipo_de_imovel from being saved:"
      %ul
        - @tipo_de_imovel.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :descricao
    = f.text_field :descricao
  .actions
    = f.submit 'Save'
