= form_for @proprietario do |f|
  - if @proprietario.errors.any?
    #error_explanation
      %h2= "#{pluralize(@proprietario.errors.count, "error")} prohibited this proprietario from being saved:"
      %ul
        - @proprietario.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :nome
    = f.text_field :nome
  .field
    = f.label :endereco
    = f.text_field :endereco
  .field
    = f.label :cpf
    = f.text_field :cpf
  .field
    = f.label :telefone
    = f.text_field :telefone
  .field
    = f.label :email
    = f.text_field :email
  .actions
    = f.submit 'Save'
